import_sal <- function(filename) {
dataframe = read_excel(filename)
dataframe = dataframe %>%
mutate(
Class = replace_na(Class,"Other"),
Year = 2000 + as.numeric(str_sub(filename,4,5))
) %>%
select(c(Class,Salary,Year))
return(dataframe)
}
